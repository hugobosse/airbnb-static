<div class="container text-center padded">
  <h1>Our team</h1>
    <ul class="list-inline text-center">
      <% data.team.each do |member| %>
        <li>
          <%= image_tag "#{member[:name]}.png", class: 'avatar-large' %>
          <h3><%= member[:name].capitalize %></h3>
        </li>
      <% end %>
    </ul>
  <h3 class="padded">
    <%= link_to ' contact@airbnb.com', 'mailto:contact@airbnb.com', class: "fa fa-envelope" %>
  </h3>
</div>
